{
  "name": "sublimity-rpc",
  "version": "0.1.0",
  "type": "module",
  "description": "Simply core implementation of pure RPC engine.",
  "keywords": [
    "typescript",
    "rpc",
    "pure"
  ],
  "author": "Kouji Matsui (@kekyo@mi.kekyo.net)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kekyo/sublimity-rpc.git"
  },
  "homepage": "https://github.com/kekyo/sublimity-rpc#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --noEmit && vite build && tsc --emitDeclarationOnly --outDir dist",
    "test": "tsc --noEmit && vitest run"
  },
  "dependencies": {
    "async-primitives": "^0.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "screw-up": "^0.3.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.0.0"
  }
}
